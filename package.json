{
  "name": "@fullstackcraftllc/codevideo-api",
  "version": "1.0.0",
  "description": "The Node express API for CodeVideo video creation.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "type-check": "tsc --noEmit",
    "test-upload": "tsc && node dist/test-upload.js",
    "clean-tmp": "rm -rf tmp/v3/new/* tmp/v3/done/* && rm -rf tmp/v3/error/* && rm -rf tmp/v3/success/* && rm -rf tmp/v3/video/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codevideo/codevideo-api.git"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "api",
    "codevideo"
  ],
  "author": "Chris Frewin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codevideo/codevideo-api/issues"
  },
  "homepage": "https://github.com/codevideo/codevideo-api#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@clerk/express": "^1.3.52",
    "@fullstackcraftllc/codevideo-types": "^2.0.39",
    "@sentry/node": "^9.5.0",
    "@sentry/profiling-node": "^7.108.0",
    "@supabase/supabase-js": "^2.39.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "isomorphic-fetch": "^3.0.0",
    "node-cron": "^3.0.3",
    "open": "^10.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
